{
  "name": "pdf",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.5.0",
  "publisher": "tomoki1207",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "pdf",
    "viewer"
  ],
  "icon": "vscode-pdf.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomoki1207/vscode-pdfviewer.git"
  },
  "bugs": {
    "url": "https://github.com/tomoki1207/vscode-pdfviewer/issues"
  },
  "activationEvents": [
    "onCustomEditor:pdf.preview"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "capabilities": {
      "untrustedWorkspaces": {
        "supported": true
      }
    },
    "customEditors": [
      {
        "viewType": "pdf.preview",
        "displayName": "%customEditor.displayName%",
        "selector": [
          {
            "filenamePattern": "*.pdf"
          }
        ]
      }
    ],
    "configuration": {
      "type": "object",
      "title": "%config.title%",
      "properties": {
        "pdf-preview.default.cursor": {
          "markdownDescription": "%config.cursor.description%",
          "type": "string",
          "default": "select",
          "enum": [
            "select",
            "hand"
          ]
        },
        "pdf-preview.default.scale": {
          "markdownDescription": "%config.scale.description%",
          "type": "string",
          "default": "auto",
          "pattern": "^(\\d+(?:\\.\\d+)?|auto|page-actual|page-fit|page-width)$"
        },
        "pdf-preview.default.sidebar": {
          "markdownDescription": "%config.sidebar.description%",
          "type": "boolean",
          "default": false
        },
        "pdf-preview.default.scrollMode": {
          "markdownDescription": "%config.scrollMode.description%",
          "type": "string",
          "default": "vertical",
          "enum": [
            "vertical",
            "horizontal",
            "wrapped"
          ]
        },
        "pdf-preview.default.spreadMode": {
          "markdownDescription": "%config.spreadMode.description%",
          "type": "string",
          "default": "none",
          "enum": [
            "none",
            "odd",
            "even"
          ]
        },
        "pdf-preview.default.nightMode": {
          "markdownDescription": "%config.nightMode.description%",
          "type": "boolean",
          "default": false
        },
        "pdf-preview.default.rememberPosition": {
          "markdownDescription": "%config.rememberPosition.description%",
          "type": "boolean",
          "default": true
        },
        "pdf-preview.statusBar.showProgress": {
          "markdownDescription": "%config.statusBar.showProgress%",
          "type": "boolean",
          "default": true
        },
        "pdf-preview.statusBar.showEstimatedTime": {
          "markdownDescription": "%config.statusBar.showEstimatedTime%",
          "type": "boolean",
          "default": true
        },
        "pdf-preview.readingStats.enabled": {
          "markdownDescription": "%config.readingStats.enabled%",
          "type": "boolean",
          "default": true
        },
        "pdf-preview.searchHistory.maxItems": {
          "markdownDescription": "%config.searchHistory.maxItems%",
          "type": "number",
          "default": 50,
          "minimum": 10,
          "maximum": 200
        }
      }
    },
